PRIVATE_KEY=
LUME_ENV= # prod or dev
LUME_API_URL=https://server-graphql-dev.up.railway.app/query

# Market maker shared config
SPREAD_BPS=100
NUM_LEVELS=20
ORDERBOOK_POLL_SECS=5

# Multi-market config (JSON file with array of markets)
# Each market entry: {"market_id": "...", "mid_price": 0.50, "total_amount": 5000}
MARKETS_CONFIG_PATH=markets_config.json

# Collateral merge config (optional - for recycling collateral)
CTF_ADDRESS=                    # CTF contract address (ERC-1155 with mergePositions)
NEG_RISK_ADAPTER=               # NegRisk adapter address (for neg risk markets)
COLLATERAL_TOKEN=               # Collateral token address (e.g., USDC)
RPC_URL=                        # RPC URL for on-chain calls
MERGE_CHECK_INTERVAL_SECS=60    # How often to check for merge opportunities
MIN_MERGE_AMOUNT=5              # Minimum shares to trigger merge

